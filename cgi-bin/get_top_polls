#!@PERL@

use strict;
use warnings;
use CGI qw(:standard);

use lib '@CGIBINDIR@';

my $home = '@CIVSDATADIR@';

my $top_polls = $home . "/elections/top_polls.html";

undef $/;

print CGI::header(-charset => 'utf-8');
# print CGI::start_html(-title => $title,
		      #-encoding => 'utf-8');

open (IN, $top_polls);

print <IN>;

exit 0;
